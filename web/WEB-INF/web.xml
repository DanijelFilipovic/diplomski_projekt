        <ejb-ref-type>Session</ejb-ref-type>
        <ejb-link>#EntityMap</ejb-link>
    </ejb-local-ref>
    <ejb-local-ref>
        <ejb-ref-name>EntityMapper1</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <ejb-link>#EntityMapper</ejb-link>
    </ejb-local-ref>
