<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.1"
			  xmlns="http://java.sun.com/xml/ns/javaee"
			  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd">
    <navigation-rule>
        <from-view-id>/*.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>skills</from-outcome>
            <to-view-id>/skills.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>licenses</from-outcome>
            <to-view-id>/licenses.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>employmentStatuses</from-outcome>
            <to-view-id>/employmentStatuses.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>organizationalUnits</from-outcome>
            <to-view-id>/organizationalUnits.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>jobs</from-outcome>
            <to-view-id>/jobs.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>customers</from-outcome>
            <to-view-id>/customers.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>trainings</from-outcome>
            <to-view-id>/trainings.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>employees</from-outcome>
            <to-view-id>/employees.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/employees.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>employee</from-outcome>
            <to-view-id>/employee.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
